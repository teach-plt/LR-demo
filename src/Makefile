	-rm -f LBNF/Abs.hs LBNF/Abs.hs.bak LBNF/ComposOp.hs LBNF/ComposOp.hs.bak LBNF/Doc.txt LBNF/Doc.txt.bak LBNF/ErrM.hs LBNF/ErrM.hs.bak LBNF/Layout.hs LBNF/Layout.hs.bak LBNF/Lex.x LBNF/Lex.x.bak LBNF/Par.y LBNF/Par.y.bak LBNF/Print.hs LBNF/Print.hs.bak LBNF/Skel.hs LBNF/Skel.hs.bak LBNF/Test.hs LBNF/Test.hs.bak LBNF/XML.hs LBNF/XML.hs.bak LBNF/AST.agda LBNF/AST.agda.bak LBNF/Parser.agda LBNF/Parser.agda.bak LBNF/IOLib.agda LBNF/IOLib.agda.bak LBNF/Main.agda LBNF/Main.agda.bak LBNF/LBNF.dtd LBNF/LBNF.dtd.bak LBNF/Test LBNF/Lex.hs LBNF/Par.hs LBNF/Par.info LBNF/ParData.hs Makefile
