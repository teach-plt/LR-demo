HAPPY_OPTS = --array --info --ghc --coerce
ALEX       = alex
ALEX_OPTS  = --ghc

# List of goals not corresponding to file names.

.PHONY : all clean distclean

# Default goal.

all : LBNF/Test

# Rules for building the parser.

LBNF/Abs.hs LBNF/Lex.x LBNF/Par.y LBNF/Print.hs LBNF/Test.hs : LBNF.cf
	bnfc --haskell -d LBNF.cf

%.hs : %.y
	${HAPPY} ${HAPPY_OPTS} $<

%.hs : %.x
	${ALEX} ${ALEX_OPTS} $<

LBNF/Test : LBNF/Abs.hs LBNF/Lex.hs LBNF/Par.hs LBNF/Print.hs LBNF/Test.hs
	${GHC} ${GHC_OPTS} $@

# Rules for cleaning generated files.

clean :
	-rm -f LBNF/*.hi LBNF/*.o LBNF/*.log LBNF/*.aux LBNF/*.dvi

